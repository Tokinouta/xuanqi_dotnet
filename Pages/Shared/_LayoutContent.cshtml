<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - xuanqi</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/xuanqi.styles.css" asp-append-version="true" />

    <link rel="apple-touch-icon" href="/static/剪贴板管理/apple-touch-ico.png">
    <link rel="shortcut icon" href="favicon.ico">
    <meta name="viewport" content="width=device-width, initial-scale=0.9, maximum-scale=0.9, user-scalable=0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css"
        integrity="sha256-93wNFzm2GO3EoByj9rKZCwGjAJAwr0nujPaOgwUt8ZQ=" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-vue@2.21.2/dist/bootstrap-vue.min.css"
        integrity="sha256-cAyk5KZc7P6j7j/uL7MOoN4PRsZYp+BN9yo03Y6Qk38=" crossorigin="anonymous">
    <style>
        html,
        body {
            max-width: 100%;
            overflow-x: hidden;
        }

        .NOBORDER {
            box-shadow: none;
            padding: 0px 10px 0px 0px;
        }

        .vertical_center {
            margin-top: 25px;
            transform: translateY(-50%);
        }

        /* ↑navbar需要 */
        .input_box {
            border: none;
            margin: 5px;
            padding: 5px;
            border-radius: 8px;
            background-color: #fff8bb;
            color: #470700;
        }

        .c-scrollbox {
            box-shadow: 0px 6px 9px rgb(218, 180, 109);
        }

        .form-control {
            border-top: 5px solid rgb(255, 212, 120)
        }

        .btn {
            font-weight: bold;
        }
    </style>
</head>

<body>
    @RenderBody()

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
